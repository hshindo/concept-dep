<Window x:Class="ConceptDep.Visualize.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ConceptDep.Visualize"
        mc:Ignorable="d"
        Title="MainWindow" Height="600" Width="1000">
	<Grid>
		<Image x:Name="Image" HorizontalAlignment="Left" Height="297" Margin="23,21,0,0" VerticalAlignment="Top" Width="935"/>
		<TextBox x:Name="Text" Height="150" Margin="0,332,0,0" TextWrapping="Wrap" VerticalAlignment="Top" SpellCheck.IsEnabled="True" VerticalScrollBarVisibility="Auto" AcceptsReturn="True" AcceptsTab="True" FontSize="21.333"/>
		<Grid HorizontalAlignment="Left" Height="100" Margin="54,300,0,0" VerticalAlignment="Top" Width="100"/>
		<Button x:Name="Update" Content="Update" HorizontalAlignment="Left" Margin="436,505,0,0" VerticalAlignment="Top" Width="90" FontSize="21.333" Click="Update_Click"/>

	</Grid>
</Window>
